			<dependency id="NEST" version="1.5.1"/>
		</dependencies>
		<tags>elasticsearch elastic search lucene nest watch watcher</tags>
	</metadata>
	<files>
		<file src="output\Nest.Watcher\net40\Nest.Watcher.dll" target="lib\net40"/>
		<file src="output\Nest.Watcher\net40\Nest.Watcher.pdb" target="lib\net40"/>
		<file src="output\Nest.Watcher\net40\Nest.Watcher.XML" target="lib\net40"/>
	</files>
</package>